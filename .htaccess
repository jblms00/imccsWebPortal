RewriteEngine On

# If the requested filename does not exist as a file

RewriteCond %{REQUEST_FILENAME} !-f

# Rewrite the URL to point to the corresponding .php file

RewriteRule ^([^\.]+)$ $1.php [NC,L]

# Custom error document for 404 Not Found

# ErrorDocument 404 http://localhost/imccsWebPortal/404
